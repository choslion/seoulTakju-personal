<main class="ko detail-page">
  <img class="detail_bori__left" src="img/move-bori_2.gif" alt="" />
  <img class="detail_bori__right" src="img/move-bori_5.gif" alt="" />
  <section class="detail-detail__wrap">
    <div class="detail-page__title">
      <h1>보도자료</h1>
      <!-- <p>서울탁주의 <span>최신소식</span>을 알려드립니다.</p> -->
    </div>
    <div class="detail-page__contents">
      <div class="detail-contents__Title">
        <input type="hidden" name="id" value="<%= row.id %> " />
        <h2><%= row.title %></h2>
        <span class="detail_writer">서울탁주</span>
        <span class="detail_writeDate"><%= row.create_time %></span>
      </div>
      <div class="detail-contents__txtWrap">
        <div class="detail-contents__text">
          <div class="detail__img">
            <img src="<%= row.img %> " alt="" />
          </div>
          <pre><%= row.content %></pre>
        </div>
      </div>
      <div class="detail-buttons">
        <a href="/news" class="detail__listBtn">목록</a>
        <div>
          <input class="detail__editBtn" type="submit" value="수정" onclick="javascript:location.href='/updateNews?id=<%= row.id %>'" />
          <input class="detail__deleteBtn" type="button" value="삭제" onclick="javascript:location.href='/deleteNews?id=<%= row.id %>'" />
        </div>
      </div>
    </div>
  </section>
</main>
