<div class="news-write__wrap">
  <form action="/newsWrite" method="post" enctype="multipart/form-data">
    <div>
      <input type="file" name="img" />
    </div>
    <div>
      <input type="text" name="title" placeholder="제목을 입력해주세요." />
    </div>
    <div>
      <textarea name="content" cols="50" rows="30" placeholder="내용을 입력해주세요."></textarea>
    </div>
    <div>
      <input type="submit" value="저장" />
    </div>
  </form>
</div>
